{
  "expo": {
    "name": "remindmee",
    "slug": "remindmee",
    "scheme": "remindmee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/remind-mee-logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/remind-mee-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#101922"
    },
    "experiments": {
      "reactCompiler": true
    },
    "ios": {
      "supportsTablet": true
    },
    "notification": {
      "icon": "./assets/remind-mee-logo.png",
      "color": "#101922"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/remind-mee-logo.png",
        "backgroundColor": "#101922"
      },
      "predictiveBackGestureEnabled": false,
      "package": "com.kenlovestocode1337.remindmee"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-community/datetimepicker",
      "expo-router",
      "expo-notifications",
      "expo-sqlite"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b3268b27-8248-4e25-af55-f4a4a4d51ece"
      }
    }
  }
}
